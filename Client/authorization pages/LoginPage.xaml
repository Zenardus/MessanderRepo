<Page x:Class="Client.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Client"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="280"
      Title="LoginPage">

    <Grid>
        <Label x:Name="label" Content="Login" HorizontalAlignment="Left" Margin="33,66,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_login" MaxLength="20" HorizontalAlignment="Left" Height="23" Margin="120,69,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_password" MaxLength="20" HorizontalAlignment="Left" Height="23" Margin="120,116,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Copy" Content="Password" HorizontalAlignment="Left" Margin="33,113,0,0" VerticalAlignment="Top"/>
    </Grid>
</Page>
